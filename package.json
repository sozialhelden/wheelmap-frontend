  "scripts": {
    "dev": "next dev",
    "build": "rimraf dist/ && rimraf .next/ && NEXT_TELEMETRY_DISABLED=1 next build",
    "start": "NODE_ENV=production next start",
    "test": "npm run test:unit && npm run test:e2e",
    "test:unit": "vitest run",
    "test:e2e": "playwright test",
    "lint": "npx biome lint",
    "lint:fix": "npx biome lint --write",
    "format": "npx biome format",
    "format:fix": "npx biome format --write",
    "transifex:push": "rm -rf src/.next/ && npx env-cmd --use-shell -- txjs-cli push ./src --token=\\$NEXT_PUBLIC_TRANSIFEX_NATIVE_TOKEN --secret=\\$TRANSIFEX_NATIVE_SECRET",
    "transifex:install": "npx api install \"@transifex/v1.0#a442qlw7q5q6u\"",
    "transifex:upload": "cd scripts && npx env-cmd -f ../.env -- npx bun upload-xliff.ts",
    "docs:build": "typedoc src --options typedoc.json --out dist/docs/typescript --json dist/typedoc-output.json && npx esrun ./tools/generate-overview.ts",
    "docs:install": "docker pull squidfunk/mkdocs-material",
    "map:style:edit": "cd scripts && npx env-cmd -f ../.env -- npx bun map-style-edit.ts",
    "map:style:upload": "cd scripts && npx env-cmd -f ../.env -- npx bun map-style-upload.ts",
    "map:style:download": "cd scripts && npx env-cmd -f ../.env -- npx bun map-style-download.ts",
    "map:style:finish": "cd scripts && npx env-cmd -f ../.env -- npx bun map-style-finish.ts",
    "prepare": "husky"
  },
  "browserslist": [
    "Android > 4.4",
    "UCAndroid >= 11.8",
    "ChromeAndroid >= 67",
    "Firefox >= 44",
    "Samsung >= 4",
    "Chrome >= 49",
    "OperaMobile > 1",
    "Safari >= 9",
    "Edge >= 16",
    "not IE 11",
    "Opera >= 53",
    "iOS >= 10",
    "last 1 version",
    "> 0.25%",
    "not dead"
  ],
  "optionalDependencies": {
    "@biomejs/cli-darwin-arm64": "^1.9.4"
  }
