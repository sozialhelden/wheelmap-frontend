  "scripts": {
    "start": "REACT_APP_TAGS_TO_APPEND_TO_INDEX_HTML_HEAD=\"\" extract=1 node scripts/start.js",
    "start-cordova": "REACT_APP_ENV=cordova REACT_APP_WHEELMAP_API_BASE_URL=https://wheelmap.org PUBLIC_URL=\"./\"",
    "analyze": "source-map-explorer build/static/js/main.*",
    "test": "node scripts/test.js --env=jsdom",
    "build": "extract=1 BABEL_DISABLE_CACHE=1 REACT_APP_TAGS_TO_APPEND_TO_INDEX_HTML_HEAD=\"\" node scripts/build.js",
    "webpack-cordova": "REACT_APP_TAGS_TO_APPEND_TO_INDEX_HTML_HEAD=\"<script src='cordova.js'></script>\" REACT_APP_ENV=development NODE_ENV=production CORDOVA=1 REACT_APP_WHEELMAP_API_BASE_URL=https://wheelmap.org PUBLIC_URL=\"./\" node scripts/build.js",
    "build-staging": "REACT_APP_TAGS_TO_APPEND_TO_INDEX_HTML_HEAD=\"\" REACT_APP_ENV=staging node scripts/build.js",
    "deploy-staging": "npm run build-staging && scp -r -P 22022 build/* rails@hulk:/var/apps/wheelmap/staging/shared/public/beta",
    "deploy-production": "npm run build && scp -r -P 22022 build/* rails@spiderman:/var/apps/wheelmap/production/shared/public/beta",
    "build-ios": "npm run webpack-cordova && rm platforms/ios/build/device/Wheelmap.ipa; bundle exec fastlane ios local --env=production",
    "build-android": "npm run webpack-cordova && rm ./platforms/android/app/build/outputs/apk/release/app-release.apk; CORDOVA_ANDROID_GRADLE_DISTRIBUTION_URL=https\\://services.gradle.org/distributions/gradle-4.4-all.zip bundle exec fastlane android local --env=production",
    "deploy-ios-beta": "rm platforms/ios/build/device/Wheelmap.ipa; npm version patch; bundle exec fastlane ios beta --env=production",
    "deploy-android-beta": "rm ./platforms/android/app/build/outputs/apk/release/app-release.apk; npm version patch; CORDOVA_ANDROID_GRADLE_DISTRIBUTION_URL=https\\://services.gradle.org/distributions/gradle-4.4-all.zip bundle exec fastlane android beta --env=production",
    "version": "cordova-version-sync",
    "flow": "flow",
    "server": "server",
    "push-translations": "tx push -s",
    "pull-translations": "tx pull -a",
    "prepublish": "npm run prepublish-babel && npm run prepublish-copy-flow-info && npmignore -u /lib",
    "prepublish-babel": "NODE_ENV=production babel --no-babelrc src/ -d lib --presets=es2015,react-app --plugins=transform-flow-strip-types --ignore '*.test.js'",
    "prepublish-copy-flow-info": "flow-copy-source src lib",
    "compile-svg-directory": "echo \"Compiling SVGs in ${basedir}, this can take up to a few minutesâ€¦\"; (for file in ${basedir}/*.svg; do ./node_modules/svgr/bin/svgr --icon $file > ${file/.svg/.js}; echo \"export { default as `basename $file .svg` } from './`basename $file .svg`';\";  done;) > ${basedir}/index.js",
    "compile-markers": "basedir=src/components/icons/markers npm run compile-svg-directory",
    "compile-equipment-icons": "basedir=src/components/icons/equipment npm run compile-svg-directory",
    "compile-category-icons": "basedir=src/components/icons/categories npm run compile-svg-directory",
    "compile-action-icons": "basedir=src/components/icons/actions npm run compile-svg-directory",
    "compile-svgs": "npm run compile-category-icons; npm run compile-markers; npm run compile-equipment-icons",
    "compile-merged-svgs": "BABEL_ENV=development babel-node --plugins transform-es2015-modules-commonjs scripts/generate-icons.js; ./node_modules/svgo/bin/svgo --multipass build/icons/*.svg"
  },
  "proxy": "https://wheelmap.org",
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {},
      "cordova-plugin-advanced-http": {},
      "cordova-plugin-wkwebview-engine": {},
      "cordova-plugin-statusbar": {},
      "cordova-plugin-device": {},
      "cordova-plugin-geolocation": {}
    },
    "platforms": [
      "ios",
      "android"
    ]
  }
}
