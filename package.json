  "scripts": {
    "dev": "nodemon --inspect --watch src/server.js --watch src/app/router.js src/server.js",
    "start": "NODE_ENV=production node src/server.js",
    "build": "rm -rf src/.next/* && next build src/",
    "analyze": "source-map-explorer build/static/js/main.*",
    "test": "node scripts/test.js --env=jsdom",
    "test-browserstack-ios": "dotenv -e .env.production bundle exec fastlane ios screenshots",
    "test-browserstack-android": "dotenv -e .env.production bundle exec fastlane android screenshots",
    "deploy-staging": "npm run build-staging && scp -r -P 22022 build/* rails@hulk:/var/apps/wheelmap/staging/shared/public/beta",
    "deploy-production": "npm run build && scp -r -P 22022 build/* rails@spiderman:/var/apps/wheelmap/production/shared/public/beta",
    "export-cordova": "REACT_APP_ENV=development NODE_ENV=production CORDOVA=1 npm run build && npx next export -o www src && grep -rl '/_next' www | xargs perl -pi -e 's/\\/_next/_next/g;'",
    "build-ios": "npm run export-cordova && rm platforms/ios/build/device/Wheelmap.ipa; bundle exec fastlane ios local --env=production",
    "build-android": "npm run export-cordova && rm -rf ./platforms/android/app/build; CORDOVA_ANDROID_GRADLE_DISTRIBUTION_URL=https\\://services.gradle.org/distributions/gradle-4.4-all.zip bundle exec fastlane android local --env=production",
    "deploy-ios-beta": "rm platforms/ios/build/device/Wheelmap.ipa; npm version patch; bundle exec fastlane ios beta --env=production",
    "deploy-android-beta": "rm -rf ./platforms/android/app/build; npm version patch; CORDOVA_ANDROID_GRADLE_DISTRIBUTION_URL=https\\://services.gradle.org/distributions/gradle-4.4-all.zip bundle exec fastlane android beta --env=production",
    "deploy-ios-release": "dotenv -e .env.production bundle exec fastlane ios publish",
    "version": "cordova-version-sync",
    "flow": "flow",
    "server": "server",
    "push-translations": "npm run build && tx push -s",
    "pull-translations": "tx pull -af",
    "retranslate": "npm run build && npm run pull-translations && NEW_SOURCE_PO='./src/static/i18n/en_US.txt' retranslate/run.sh",
    "create-js-translations": "node scripts/parseGettextFiles.js",
    "update-local-translations": "npm run pull-translations && npm run create-js-translations",
    "compile-svg-directory": "echo \"Compiling SVGs in ${basedir}, this can take up to a few minutesâ€¦\"; (for file in ${basedir}/*.svg; do ./node_modules/svgr/bin/svgr --icon $file > ${file/.svg/.js}; echo \"export { default as `basename $file .svg` } from './`basename $file .svg`';\";  done;) > ${basedir}/index.js",
    "compile-markers": "basedir=src/components/icons/markers npm run compile-svg-directory",
    "compile-equipment-icons": "basedir=src/components/icons/equipment npm run compile-svg-directory",
    "compile-category-icons": "basedir=src/components/icons/categories npm run compile-svg-directory",
    "compile-action-icons": "basedir=src/components/icons/actions npm run compile-svg-directory",
    "compile-svgs": "npm run compile-category-icons; npm run compile-markers; npm run compile-equipment-icons",
    "compile-merged-svgs": "BABEL_ENV=development babel-node --plugins transform-es2015-modules-commonjs scripts/generate-icons.js; ./node_modules/svgo/bin/svgo --multipass build/icons/*.svg",
    "format": "prettier-eslint --config .prettierrc \"src/**/*.js\" --write"
  },
  "babel": {
    "presets": [
      "next/babel"
    ],
    "plugins": [
      "transform-flow-strip-types",
      [
        "ttag",
        {
          "extract": {
            "output": "public/i18n/translations.pot"
          }
        }
      ],
      "babel-plugin-styled-components"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{src,test}/**/*.{js,jsx}": [
      "prettier-eslint --write ",
      "git add"
    ]
  },
  "browserslist": [
    "Android > 4.4",
    "UCAndroid >= 11.8",
    "ChromeAndroid >= 67",
    "Firefox >= 44",
    "Samsung >= 4",
    "Chrome >= 49",
    "OperaMobile > 1",
    "Safari >= 9",
    "Edge >= 16",
    "IE >= 11",
    "Opera >= 53",
    "iOS >= 10",
    "last 1 version",
    "> 1%"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {},
      "cordova-plugin-advanced-http": {},
      "cordova-plugin-wkwebview-engine": {},
      "cordova-plugin-statusbar": {},
      "cordova-plugin-device": {},
      "cordova-plugin-geolocation": {}
    },
    "platforms": [
      "ios",
      "android"
    ]
  }
}
